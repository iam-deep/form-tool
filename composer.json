{
    "name": "iam-deep/form-tool",
    "description": "A lightweight Laravel form tool to create your admin panel or web app easily!",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Deep\\FormTool\\": "src/"
        },
        "files": [
          "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Biswadeep Sarkar",
            "email": "biswadeepoffice@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "intervention/image" : "^2.7.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deep\\FormTool\\FormToolServiceProvider"
            ]
        }
    }
}
